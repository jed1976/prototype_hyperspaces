  "title": "Strict Not-Equal",
  "description": "Tests strict not-equal. Requires two arguments.",

  "type": "object",
  "additionalProperties": false,
  "required": ["!=="],
  "properties": {
    "!==": {
      "$ref": "http://jsonlogic.com/schemas/common/binary-args.json"
